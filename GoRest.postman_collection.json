			"name": "1-CreateAUser",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const response=pm.response.json();\r",
							"const userId=response.id;\r",
							"console.log(\"user Id is : \"+userId );\r",
							"\r",
							"//C , E, G , L\r",
							"//set userid as a local variable\r",
							"pm.variables.set(\"user_id\", userId);\r",
							"\r",
							"//Test Cases\r",
							"pm.test(\"Status code is 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Authorization",
						"value": "{{tokenId}}",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n        \"name\": \"{{$randomFullName}}\",\r\n        \"email\": \"Test_{{$randomEmail}}\",\r\n        \"gender\": \"female\",\r\n        \"status\": \"active\"\r\n    }",
					"options": {
